<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.company.supershop.model.PromotionString" table="promotion_string" lazy="false">
        <id name="id" type="int">
            <column name="id" />
            <generator class="increment" />
        </id>
        <property name="promotionId" type="int">
            <column name="promotion_id" />
        </property>
        <property name="string" type="string">
            <column name="string" />
        </property>
        <property name="dateCreated" type="date">
            <column name="date_created" length="13" not-null="true" />
        </property>
        <property name="dateUpdated" type="date">
            <column name="date_updated" length="13" not-null="true" />
        </property>
    </class>
	
</hibernate-mapping>
